public class Solution {
    public int solve(String A) {
        A = A.toUpperCase();
        int sum = 0;
        for(int i=0;i<A.length();i++){
            char c = A.charAt(i);
            if(c == 'A'||c == 'O'||c == 'I'||c == 'E'||c == 'U'){
                sum += A.length()-i;
            }
        }
        return sum % 10003;
    }
}
